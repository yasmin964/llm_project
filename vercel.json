{
  "functions": {
    "api/bot.py": {
      "runtime": "python3.10",
      "maxDuration": 10,
      "memory": 1024
    }
  },
  "builds": [
    {
      "src": "api/bot.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/bot",
      "dest": "/api/bot.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/bot.py"
    }
  ]
}